{
  "name": "@josephuspaye/explorer-file-ops-cli",
  "version": "0.1.0",
  "description": "Copy, move, and delete files and folders using the File Explorer GUI from the command line",
  "main": "dist/main.js",
  "repository": "https://github.com/JosephusPaye/explorer-file-ops-cli.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "ecp": "dist/copy.js",
    "emv": "dist/move.js",
    "erm": "dist/delete.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"*.{js,json,md}\"",
    "test": "echo no automated tests"
  },
  "dependencies": {
    "@josephuspaye/explorer-file-ops": "^0.1.0",
    "kleur": "^4.1.3",
    "sade": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/sade": "^1.7.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
